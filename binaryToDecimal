package Lecture3;

public class BinaryToDecimal {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		int bin = 100000;

		int dec = 0;
		int twopowers = 1;// 2 raise to power 0.
		while (bin != 0) {
			int rem = bin % 10;
			dec = dec + rem * twopowers;
			bin = bin/ 10;
			twopowers = twopowers * 2;
		}

		System.out.println(dec);
	}

}
